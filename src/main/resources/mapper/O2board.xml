<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.kosta.o2dao.O2DAO">
  <insert id="twriteinsert" parameterType="com.kosta.o2dao.O2DAO">
  	insert into TradeBoard_3jo(user_id,title,content
  								,category,like,writedate
  								,file_name,deal,deal_price
  								,map_x,map_y,tradeaddr
  								,viewno)
  	values('kim',${title},${content},'test',0,sysdate()
  			,'test',0,0
  			,${map_x},${map_y},'서울 구로구'
  			,0)
  </insert>
  
  <select id="getlist" resultType="com.kosta.dto.O2QnaBoardDTO">
    select qnano,
           user_id,
           content,
           category,
           writedate
           from QABoard_3jo;
  </select>
  
  </mapper>